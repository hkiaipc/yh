      </sendpart>

      <receivepart length="39">
        <datafield name="address"
                   begin="0"
                   length="1"
                   converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="fc"
                   begin="1"
                   length="1"
                   converter="Xdgk.Communi.Int8Converter"
                   bytes="04"
                   matchcheck="true"/>

        <datafield name="indatalength"
                   begin="2"
                   length="1"
                   converter="Xdgk.Communi.Int8Converter"
                   bytes="22"
                   matchcheck="true"/>

        <datafield name="IF"
                   begin="3"
                   length="4"
                   converter="Xdgk.Communi.UInt32ModbusConverter"/>

        <datafield name="RemainAmount"
                   begin="7"
                   length="4"
                   converter="Xdgk.Communi.UInt32ModbusConverter"/>

        <datafield name="Height"
                   begin="11"
                   length="2"
                   converter="Xdgk.Communi.UInt16Converter"
                   littleendian="false"/>

        <datafield name="WL2"
                   begin="25"
                   length="2"
                   converter="Xdgk.Communi.Int16Converter"
                   littleendian="false"/>

        <datafield name="WL1"
                   begin="35"
                   length="2"
                   converter="Xdgk.Communi.Int16Converter"
                   littleendian="false"/>

        <datafield name="crc"
                   begin="37"
                   length="2"
                   converter="Xdgk.Communi.OriginalConverter"
                   crc="true"/>

        <crcer name="Xdgk.Communi.CRC16"/>
      </receivepart>
    </operadefine>

  </devicedefine>
</devicedefines>
